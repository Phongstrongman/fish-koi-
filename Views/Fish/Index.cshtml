@model List<Fish>

<h2>Danh sách cá</h2>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Tên</th>
            <th>Loại</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var fish in Model)
        {
            <tr>
                <td>@fish.Id</td>
                <td>@fish.Name</td>
                <td>@fish.Species</td>
            </tr>
        }
    </tbody>
</table>
